{
  "name": "maidr-ts",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "eslint '**/*.ts' --fix",
    "clean": "rm -rf dist",
    "commitlint": "commitlint --from=HEAD~1 --to=HEAD",
    "compile": "tsc",
    "cypress:run": "cypress run",
    "prepare": "husky",
    "pretest": "npm run compile",
    "posttest": "npm run lint || true",
    "start-server": "http-server -p 8080",
    "test": "start-server-and-test start-server http://localhost:8080 cypress:run"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@cypress/webpack-preprocessor": "^6.0.2",
    "@eslint/js": "^9.19.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.12.0",
    "@types/webpack": "^5.28.5",
    "cypress": "^14.0.1",
    "eslint": "^9.19.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "start-server-and-test": "^2.0.10",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "cypress-real-events": "^1.14.0",
    "hotkeys-js": "^3.13.9",
    "zod": "^3.24.1"
  }
}
